<script>
    if (localStorage.getItem("persist-view") === "grid-view") {
        $('.product-grid').addClass('grid-view');
        $('.product-grid').removeClass('list-view list-view-large');
    }

    if (localStorage.getItem("persist-view") === "list-view") {
        $('.product-grid').addClass('list-view');
        $('.product-grid').removeClass('grid-view list-view-large');
    }

    if (localStorage.getItem("persist-view") === "classic-view") {
        $('.product-grid').addClass('list-view list-view-large');
        $('.product-grid').removeClass('grid-view');
    }
</script>




